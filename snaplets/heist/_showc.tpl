<div id="sidebar">
  <div class="panel panel-default quick-facts">
    <div class="panel-heading">
      <h3 class="panel-title">Quick Facts</h3>
    </div>
    <div class="panel-body">
      <ul>
	<prop-links>
	  <li><b><prop-name /></b><a class="${prop}"><prop-val /></a></li>
	</prop-links>
	<info-spans>
	  <li><b><info-name /></b><span class="${class}">
	    <info-values>
	      <span><info-value /></span>
	    </info-values>
	  </span>
	  </li>
	</info-spans>
      </ul>
    </div>
  </div>
</div>
